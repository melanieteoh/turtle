from turtle import *
exitonclick()
speed(50)
width(10)

counter = 1
while counter <= 1:
    color("pink")
    left(45)
    forward(77)
    left(45)
    forward(35)
    left(45)
    forward(25)
    left(45)
    forward(20)
    left(45)
    forward(25)
    right(90)
    forward(25)
    left(45)
    forward(20)
    left(45)
    forward(25)
    left(45)
    forward(35)
    left(45)
    forward(77)
    left(90)
    counter += 1
    
